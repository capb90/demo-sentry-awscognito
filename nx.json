{
  "npmScope": "amplify-app",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "auth": {
      "tags": []
    },
    "auth-e2e": {
      "tags": [],
      "implicitDependencies": ["auth"]
    },
    "demo-amplify": {
      "tags": []
    },
    "demo-amplify-e2e": {
      "tags": [],
      "implicitDependencies": ["demo-amplify"]
    }
  }
}
