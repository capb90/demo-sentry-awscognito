{
  "version": 1,
  "projects": {
    "auth": {
      "projectType": "library",
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "prefix": "amplify-app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/auth"],
          "options": {
            "jestConfig": "libs/auth/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/auth/src/**/*.ts",
              "libs/auth/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/auth/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/auth",
            "config": {
              "configFolder": "libs/auth/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "auth-e2e": {
      "root": "apps/auth-e2e",
      "sourceRoot": "apps/auth-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/auth-e2e/cypress.json",
            "tsConfig": "apps/auth-e2e/tsconfig.e2e.json",
            "devServerTarget": "auth:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "auth:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/auth-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "demo-amplify": {
      "projectType": "application",
      "root": "apps/demo-amplify",
      "sourceRoot": "apps/demo-amplify/src",
      "prefix": "amplify-app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo-amplify",
            "index": "apps/demo-amplify/src/index.html",
            "main": "apps/demo-amplify/src/main.ts",
            "polyfills": "apps/demo-amplify/src/polyfills.ts",
            "tsConfig": "apps/demo-amplify/tsconfig.app.json",
            "assets": [
              "apps/demo-amplify/src/favicon.ico",
              "apps/demo-amplify/src/assets"
            ],
            "styles": ["apps/demo-amplify/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo-amplify/src/environments/environment.ts",
                  "with": "apps/demo-amplify/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo-amplify:build:production"
            },
            "development": {
              "browserTarget": "demo-amplify:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-amplify:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/demo-amplify/src/**/*.ts",
              "apps/demo-amplify/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo-amplify"],
          "options": {
            "jestConfig": "apps/demo-amplify/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-amplify-e2e": {
      "root": "apps/demo-amplify-e2e",
      "sourceRoot": "apps/demo-amplify-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-amplify-e2e/cypress.json",
            "tsConfig": "apps/demo-amplify-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo-amplify:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-amplify:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo-amplify-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "demo-amplify"
}
